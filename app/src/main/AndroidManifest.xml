<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.ivanfenenko.tablebooking">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="me.ivanfenenko.tablebooking.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="me.ivanfenenko.tablebooking.customers.UserListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:parentActivityName="me.ivanfenenko.tablebooking.customers.UserListActivity"
            android:name="me.ivanfenenko.tablebooking.tables.TableBookingActivity"/>
    </application>

</manifest>